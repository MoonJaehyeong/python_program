#Qz1
genres_tuple = ("pop", "rock", "soul", "hard rock", "soft rock", \
                "R&B", "progressive rock", "disco") 

len(genres_tuple)
#result: 8

#Qz2
genres_tuple[3]
#result: 'hard rock'

#Qz3
genres_tuple[3:6]
#result: ('hard rock', 'soft rock', 'R&B')

#Qz4
genres_tuple[0:2]
#result: ('pop', 'rock')

#Qz5
genres_tuple.index("disco")
#result: 7

